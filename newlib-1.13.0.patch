+#ifdef F__read
+int _read(int fd, void *buf, size_t size)
+{
+	if (fd < 0) {
+		errno = EBADF;
+		return -1;
+	}
+
+	return __psp_set_errno(sceIoRead(fd, buf, size));
+}
+#endif
+
+#ifdef F__write
+int _write(int fd, const void *buf, size_t size)
+{
+	if (fd < 0) {
+		errno = EBADF;
+		return -1;
+	}
+
+	return __psp_set_errno(sceIoWrite(fd, buf, size));
