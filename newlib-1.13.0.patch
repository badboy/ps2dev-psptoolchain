diff -burN newlib-1.13.0/newlib/libc/sys/psp/pspcwd.c newlib-psp/newlib/libc/sys/psp/pspcwd.c
--- newlib-1.13.0/newlib/libc/sys/psp/pspcwd.c	1970-01-01 01:00:00.000000000 +0100
+++ newlib-psp/newlib/libc/sys/psp/pspcwd.c	2005-12-07 20:10:58.000000000 +0000
@@ -0,0 +1,167 @@
+/*
+ * PSP Software Development Kit - http://www.pspdev.org
+ * -----------------------------------------------------------------------
+ * Licensed under the BSD license, see LICENSE in PSPSDK root for details.
+ *
+ * pspcwd.c - Current working directory emulation helper functions
